<?xml version="1.0" encoding="utf-8"?>
<com.google.customviewanalysis.ItemCustomView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:padding="5dp"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/gk_img"
        android:src="@drawable/pngegg"
        android:layout_width="100dp"
        android:layout_height="100dp"/>
    <TextView
        android:id="@+id/tv_1"
        android:maxLines="1"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:layout_width="150dp"
        android:layout_height="50dp" />
    <ImageView
        android:id="@+id/img_1"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:layout_height="25dp"/>
    <ImageView
        android:id="@+id/img_2"
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:layout_height="25dp"/>
    <TextView
        android:id="@+id/dummy"
        android:layout_width="match_parent"
        android:maxLines="1"
        android:gravity="center_vertical"
        android:ellipsize="end"
        android:layout_height="25dp"/>
    <Button
        android:layout_toEndOf="@+id/gk_img"
        android:id="@+id/btn"
        android:layout_width="170dp"
        android:maxLines="1"
        android:ellipsize="end"
        android:layout_height="50dp" />
    <TextView
        android:id="@+id/neutral"
        android:layout_width="match_parent"
        android:maxLines="1"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:layout_height="50dp"/>

</com.google.customviewanalysis.ItemCustomView>